{
	"author": "Alex Kahn",
	"bin": {
		"absolutejs": "./dist/cli/index.js"
	},
	"description": "A fullstack meta-framework for building web applications with TypeScript",
	"engines": {
		"bun": ">=1.3.6"
	},
	"devDependencies": {
		"@elysiajs/static": "1.4.0",
		"@stylistic/eslint-plugin-ts": "4.2.0",
		"@tailwindcss/cli": "4.1.7",
		"@types/bun": "1.3.9",
		"@types/react": "19.2.0",
		"@types/react-dom": "19.2.0",
		"autoprefixer": "10.4.21",
		"elysia": "1.4.18",
		"elysia-scoped-state": "0.1.1",
		"eslint": "9.26.0",
		"eslint-plugin-absolute": "0.1.4",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-security": "3.0.1",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"react": "19.2.1",
		"react-dom": "19.2.1",
		"svelte": "5.35.2",
		"tailwindcss": "4.1.7",
		"typescript": "5.8.3",
		"typescript-eslint": "8.32.0",
		"vue": "3.5.27"
	},
	"dependencies": {
		"react-refresh": "^0.18.0"
	},
	"license": "BSL-1.1",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/src/react/index.d.ts",
			"import": "./dist/react/index.js"
		},
		"./react/hooks": {
			"types": "./dist/src/react/hooks/index.d.ts",
			"import": "./dist/react/hooks/index.js"
		},
		"./svelte": {
			"types": "./dist/src/svelte/index.d.ts",
			"import": "./dist/svelte/index.js"
		},
		"./vue": {
			"types": "./dist/src/vue/index.d.ts",
			"import": "./dist/vue/index.js"
		}
	},
	"main": "./dist/index.js",
	"name": "@absolutejs/absolute",
	"peerDependencies": {
		"elysia": "^1.4.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"svelte": "^5.35.2",
		"vue": "^3.5.27"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"svelte": {
			"optional": true
		},
		"vue": {
			"optional": true
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/absolutejs/absolutejs.git"
	},
	"scripts": {
		"build": "rm -rf dist && bun build src/index.ts src/react/index.ts src/react/hooks/index.ts src/svelte/index.ts src/vue/index.ts --outdir dist --sourcemap --target=bun --external react --external react-dom --external vue --external @vue/compiler-sfc --external vue/server-renderer --external svelte --external svelte/compiler --external svelte/server --external elysia && bun build src/cli/index.ts --outfile dist/cli/index.js --target=bun && tsc --emitDeclarationOnly --project tsconfig.build.json && mkdir -p dist/dev && cp -r src/dev/client dist/dev/client",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"dev": "bun run src/cli/index.ts dev example/server.ts",
		"start": "NODE_ENV=production bun run example/server.ts",
		"format": "bun run src/cli/index.ts prettier --write",
		"lint": "bun run src/cli/index.ts eslint",
		"release": "bun run format && bun run build && bun publish",
		"test": "bun test",
		"typecheck": "bun run tsc --noEmit"
	},
	"types": "./dist/src/index.d.ts",
	"version": "0.16.12"
}
